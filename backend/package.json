{
  "name": "backend-rest-api-template",
  "version": "1.0.0",
  "type": "module",
  "description": "Full Express Production grade setup for rest-apis",
  "main": "./src/server.js",
  "scripts": {
    "lang": "node src/config/langchain/langchain_setup.js",
    "dev": "  nodemon src/server.js --development",
    "start": "  node src/server.js --production",
    "cm": "node scripts/createModule.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "prepare": "node -e \"if (process.env.NODE_ENV !== 'production') require('husky')\"",
    "seed": "node src/config/seeding.js"
  },
  "keywords": [
    "Express",
    "template"
  ],
  "author": "dhirajsuthar781",
  "license": "MIT",
  "dependencies": {
    "@faker-js/faker": "^9.9.0",
    "@langchain/classic": "^1.0.7",
    "@langchain/core": "^1.1.8",
    "@langchain/google-genai": "^2.1.3",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.3",
    "langchain": "^1.2.3",
    "mongoose": "^8.20.4",
    "morgan": "^1.10.0",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.3",
    "nodemon": "^3.1.11",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "prettier": "^3.6.2"
  }
}
